<Alloy>
  <ListView id="table" defaultItemTemplate="row">
    <Templates>
      <ItemTemplate name="row">
        <View bindId="cell" id="cell">
          <ImageView bindId="image" id="image"/>
          <Label bindId="title" id="title"/>
          <Label bindId="summary" id="summary"/>
          <Label bindId="origin" id="origin"/>
          <Label bindId="state" id="state"/>
          <Label bindId="ago" id="ago"/>
        </View>
      </ItemTemplate>
    </Templates>
    <ListSection/>
  </ListView>
</Alloy>
