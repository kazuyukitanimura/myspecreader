<Alloy>
  <TableViewRow onClick="openSummary">
    <View id="cell">
      <ImageView id="image" image="{img}"/>
      <Label id="title" text="{title}"/>
      <Label id="summary" text="{summary}"/>
      <Label id="ago" text="{ago}"/>
      <Label id="origin" text="{origin}"/>
    </View>
  </TableViewRow>
</Alloy>
